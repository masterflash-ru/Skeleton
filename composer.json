{
    "name": "masterflash-ru/skeleton",
    "description": "Стартовое приложение для создания нового сайта",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "simba",
        "mvc",
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3 || ^7.4",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.1.0",
        "zfcampus/zf-development-mode": "^3.2",
        "zendframework/zend-session": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "extra": {
    },
    "scripts": {
        "post-update-cmd": "rm -rf ./data/cache/*",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ]
    }
}
